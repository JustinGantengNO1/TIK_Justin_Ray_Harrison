//array

#include <iostream>
using namespace std;

int main(){
    int angka [] = {
    };
       for (int j = 1; j<6 ; j++){
           cout<<"Masukkan angka "<<j<<" = "; cin>>angka[j];
           
    }
    
    for (int i = 1; i<6 ; i++){
        cout<<"Angka ke - "<<i<<" = "<<angka[i]*angka[i]<<endl;
    }
    return 0;
}
